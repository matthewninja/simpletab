{
  "manifest_version": 2,

  "name": "Simple Tab",
  "description": "Transforms your new tab page into a beautiful and minimalistic image.\n All code is open source.",
  "version": "0.1",
  "background": {
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  // "options_page": "options.html",
  "chrome_url_overrides" : {
    "newtab": "new-tab.html"
  },
  "permissions": [
    "tabs"
  ]
}